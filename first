function getValueFromSet(state, set, name) {
 for (var i = 0;i < set.Set.Data.length;i++) {
 var v = set.Set.Data[i];
 var v = set.Set.Data[i];
 if (v.Name == name) {
 return v.Value;
 }
 }
 
 throw name + " not found in data";
}
